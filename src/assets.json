{
  "defaults": {
    "js": {
      "regex": "js",
      "prepend": "dist/app/"
    },
    "css": {
      "regex": "css",
      "prepend": "dist/app/"
    },
    "prettyPrint": true,
    "pathFromRoot": "./src/assets.json"
  },
  "config": {
    "myApp": {
      "webpackEntry": "myApp",
      "target": [
        "./tests/template.html",
        "./tests2/template.html"
      ],
      "assets": {
        "js": {
          "sourcePaths": [
            "dist/app/myApp-38ee7de36e7d8f3f0b97.js"
          ],
          "regex": "myjs"
        },
        "css": {
          "sourcePaths": [
            "myApp-3c175a36bc1302859163.css"
          ],
          "regex": "mycss"
        }
      }
    }
  }
}